# cluster definition
ENVIRONMENT_NAME=stam
BOOTSTRAP=SSL://node-4.intel.r7g.org:9093,SSL://node-5.intel.r7g.org:9093,SSL://node-6.intel.r7g.org:9093

# cluster connection
SECURITY_PROTOCOL=SSL
#SASL_MECHANISM=
# keep as is, otherwise not working
#SASL_JAAS_CONFIG=
SSL_ENDPOINT_IDENTIFICATION_ALGORITHM=
SSL_PROTOCOL=TLS
SSL_KEYSTORE_LOCATION=/data/certificate/users/admin/admin.jks
