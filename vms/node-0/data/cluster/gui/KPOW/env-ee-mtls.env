# cluster definition
ENVIRONMENT_NAME=stam
BOOTSTRAP=SSL://node-4.intel.r7g.org:9093,SSL://node-5.intel.r7g.org:9093,SSL://node-6.intel.r7g.org:9093

# cluster connection
SECURITY_PROTOCOL=SSL
#SASL_MECHANISM=
# keep as is, otherwise not working
#SASL_JAAS_CONFIG=
SSL_ENDPOINT_IDENTIFICATION_ALGORITHM=
SSL_PROTOCOL=TLSv1.3
SSL_KEYSTORE_LOCATION=/data/cluster/certificates/users/admin/admin.jks
SSL_KEYSTORE_PASSWORD=insecure
SSL_KEY_PASSWORD=insecure
SSL_TRUSTSTORE_LOCATION=/data/cluster/certificates/shared.truststore.jks
SSL_TRUSTSTORE_PASSWORD=insecure

# connect
# Configure Connect
CONNECT_NAME=Connect Cluster
CONNECT_REST_URL=https://node-0.intel.r7g.org:8080
CONNECT_PERMISSIVE_SSL=true
CONNECT_SSL_KEYSTORE_LOCATION=/data/cluster/certificates/users/admin/admin.jks
CONNECT_SSL_KEYSTORE_PASSWORD=insecure
CONNECT_SSL_KEYSTORE_TYPE=JKS
CONNECT_SSL_TRUSTSTORE_LOCATION=/data/cluster/certificates/shared.truststore.jks
CONNECT_SSL_TRUSTSTORE_PASSWORD=insecure
CONNECT_SSL_TRUSTSTORE_TYPE=JKS
CONNECT_AUTO_RESTART="*"

# default serders
DEFAULT_HEADERS_SERDES=JSON
DEFAULT_KEY_SERDES=JSON
DEFAULT_VALUE_SERDES=JSON

